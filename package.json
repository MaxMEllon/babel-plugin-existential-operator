{
  "name": "babel-plugin-transform-isnil",
  "version": "0.0.1",
  "description": "replace the comparing of null or undefined with isNil",
  "main": "lib",
  "scripts": {
    "compile": "babel -V && babel src --out-dir lib",
    "pretest": "npm run compile",
    "test": "xo --ignore lib/*.js && ava",
    "fix": "xo --ignore lib/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxMEllon/babel-plugin-existential-operator.git"
  },
  "keywords": [
    "babel-plugin"
  ],
  "xo": {
    "semicolon": false,
    "space": 2
  },
  "author": "MaxMEllon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaxMEllon/babel-plugin-existential-operator/issues"
  },
  "homepage": "https://github.com/MaxMEllon/babel-plugin-existential-operator#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "ava-spec": "^1.0.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "xo": "^0.15.0"
  },
  "dependencies": {
    "flat": "^2.0.0"
  }
}
